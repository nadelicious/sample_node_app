	<h2 class="form-signin-heading">Please Sign Up</h2>
	<input type="text" name="username" placeholder="Username" class="input-block-level" required autocomplete="off">
	<input type="password" name="password" placeholder="Password" class="input-block-level" required autocomplete="off">
	<input type="email" name="email" placeholder="Email  address" class="input-block-level" required autocomplete="off">
	<input type="text" name="location" placeholder="Location" class="input-block-level" required autocomplete="off">
	<input type="submit" class="btn btn-large btn-primary" value="submit">
</form>
